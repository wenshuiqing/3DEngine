#include <iostream>
#include "IApplication.hpp"

namespace My{
	extern IApplication* g_pApp;
}

int main(int argc,char** argc){
	int ret = 0;
	
	if((ret=g_pApp->Init())!=0){
		std::cout<<"init is fail"<<std::endl;
		return ret;
	}

	while(!g_pApp->IsQuit()){
		g_App->Update();
	}

	g_App->Destroy();

	return 0;

}	
